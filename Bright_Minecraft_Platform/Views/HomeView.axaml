<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Bright_Minecraft_Platform.Views.HomeView">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="70"/>
		</Grid.RowDefinitions>
		<WrapPanel Grid.Row="1" Margin="10 0  10 0" VerticalAlignment="Center" HorizontalAlignment="Right">
			<Button Margin="0 0 10 0" Content="{Binding Selectedaccount}" Width="150" Height="50" CornerRadius="6" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
			<ComboBox Width="150" Height="50" Margin="0 0 10 0" CornerRadius="6"/>
			<Button Content="{Binding Launchgame}" Width="150" Height="50" CornerRadius="6" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
		</WrapPanel>
	</Grid>
</UserControl>
